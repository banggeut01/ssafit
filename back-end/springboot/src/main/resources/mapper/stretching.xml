<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ssafy.stretching">

	<select id="getVideoPath"
		resultType="com.ssafy.edu.vue.dto.Stretching">
		select title, file
		from stretching
		where stretching_id=#{num}
	</select>

	
</mapper>
